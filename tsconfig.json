{
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM"
    ],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "types": [
      "bun-types"
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "skills",
    ".agent",
    ".opencode"
  ]
}